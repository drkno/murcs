<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sws.murcs.controller.editor.EffortController">
   <children>
      <ScrollPane hbarPolicy="NEVER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox alignment="TOP_RIGHT" prefHeight="290.0" prefWidth="490.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox spacing="5.0">
                     <children>
                        <DatePicker fx:id="datePicker" prefWidth="100.0" />
                        <ComboBox fx:id="personChoiceBox" prefWidth="100.0" promptText="person" />
                        <TextField fx:id="timeTextField" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="50.0" promptText="time" />
                        <TextField fx:id="descriptionTextField" promptText="description" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#addButtonClick" text="Add" />
                  <Separator prefWidth="200.0" />
                  <VBox fx:id="effortsVBox" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </content>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ScrollPane>
   </children>
</AnchorPane>
