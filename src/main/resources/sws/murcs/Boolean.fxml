<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sws.murcs.magic.easyedit.fxml.BasicBooleanEditController">
   <children>
      <Text fx:id="titleText" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
      <HBox spacing="10">
         <children>
            <RadioButton fx:id="trueButton" mnemonicParsing="false" text="True" />
            <RadioButton fx:id="falseButton" mnemonicParsing="false" text="False" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
