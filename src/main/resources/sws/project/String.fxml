<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sws.project.magic.easyedit.fxml.BasicStringEditController">
   <children>
       <GridPane>
       <columnConstraints>
           <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
           <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       </columnConstraints>
       <rowConstraints>
           <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
       </rowConstraints>
       <children>
           <Text fx:id="invalidNode" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="Invalid Text!" GridPane.columnIndex="1" GridPane.halignment="RIGHT" visible="false"/>
           <Text fx:id="titleText" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
       </children>
   </GridPane>
      <TextField fx:id="valueText" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
