<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sws.project.controller.TeamEditor">
   <children>
      <ScrollPane fitToWidth="true" prefViewportHeight="380.0" prefViewportWidth="169.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox spacing="10" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <VBox>
                     <children>
                        <Label text="Name" />
                        <TextField fx:id="nameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                      <padding>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                      </padding>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Long Name" GridPane.rowIndex="1" />
                        <TextField fx:id="longNameTextField" GridPane.columnIndex="1" />
                     </children>
                      <padding>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                      </padding>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Description" GridPane.rowIndex="1" />
                        <TextField fx:id="descriptionTextField" GridPane.columnIndex="1" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Product Owner" />
                        <AnchorPane>
                           <children>
                              <ComboBox fx:id="productOwnerPicker" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                           </children>
                        </AnchorPane>
                     </children>
                      <padding>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                      </padding>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Scrum Master" />
                        <AnchorPane>
                           <children>
                              <ComboBox fx:id="scrumMasterPicker" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" VBox.vgrow="ALWAYS" />
                           </children>
                        </AnchorPane>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Team Members" GridPane.rowIndex="1" />
                        <VBox fx:id="teamMembersContainer" VBox.vgrow="ALWAYS" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add a member">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Text>
                        <AnchorPane>
                           <children>
                              <ComboBox fx:id="addTeamMemberPicker" prefWidth="150.0" />
                           </children>
                        </AnchorPane>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </VBox>
                   <Label fx:id="labelErrorMessage" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
